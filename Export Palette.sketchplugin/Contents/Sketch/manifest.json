{
	"name": "Export Colors",
	"description": "Exports your Sketch palette as rgb(a) to CSS, SCSS, uiColor, and XML",
	"author": "Wil Nichols",
	"authorEmail": "wil@zello.com",
	"version": "0.0.0",
	"identifier": "com.sketch.export-palette",
	"bugs": {
		"url": "https://github.com/wilnichols/Sketch-Color-Vars/issues"
	},
	"homepage": "https://github.com/wilnichols/Sketch-Color-Vars",
	"appcast": "https://raw.githubusercontent.com/wilnichols/Sketch-Color-Vars/master/appcast.xml",
	"license": "MIT",
	"commands": [
		{
			"script": "generate.cocoascript",
			"shortcut": "shift cmd 0",
			"name": "Generate Swatches from Selection",
			"handlers": {
				"run": "onRun"
			},
			"identifier": "generate"
		},
		{
			"script": "androidcolors.cocoascript",
			"shortcut": "shift cmd 9",
			"name": "Export for Android",
			"handlers": {
				"run": "onRun"
			},
			"identifier": "androidcolors"
		},
		{
			"script": "webcolors.cocoascript",
			"shortcut": "shift cmd -",
			"name": "Export for Web",
			"handlers": {
				"run": "onRun"
			},
			"identifier": "webcolors"
		},
		{
			"script": "csvcolors.cocoascript",
			"shortcut": "shift cmd =",
			"name": "Export to CSV",
			"handlers": {
				"run": "onRun"
			},
			"identifier": "csvcolors"
		},
		{
			"script": "origamicolors.cocoascript",
			"shortcut": "",
			"name": "Export to Origami (JSON)",
			"handlers": {
				"run": "onRun"
			},
			"identifier": "origamicolors"
		}
	],
	"menu": {
		"items": [
			"generate",
			"-",
			"androidcolors",
			"webcolors",
			"csvcolors",
			"origamicolors"
		],
		"title": "Export Colors"
	}
}